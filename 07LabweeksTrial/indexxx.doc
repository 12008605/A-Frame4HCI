<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>LabWeeks</title>
    <meta name="viewport" content="width=device-width">
    <script src="https://aframe.io/releases/1.0.4/aframe.min.js"></script>
    <script src="https://unpkg.com/aframe-environment-component@1.1.0/dist/aframe-environment-component.min.js"></script>
    <script src="scripts/main.js"></script>
    <script src="https://codechangers.com/physics.js"></script>
    <script src="https://codechangers.com/shoot-full.js"></script>
  </head>
  <body>
    <a-scene id="myScene">
      <a-camera id="myCamera" pointer-lock-enabled="true" position="0 1 0">
         <a-cursor fuse="true"></a-cursor>
      </a-camera>
      <a-box id="myBox" static-body position="0 0.5 -7" rotation="0 0 0" width="9" color="green"></a-box>
      <a-sphere dynamic-body radius="0.5" position="0 1.5 -7" color="#9FD8EF"></a-sphere>
      <a-sphere dynamic-body radius="0.5" position="-2 1.5 -7" color="#9FD8EF"></a-sphere>
      <a-sphere dynamic-body radius="0.5" position="-4 1.5 -7" color="#9FD8EF"></a-sphere>
      <a-sphere dynamic-body radius="0.5" position="2 1.5 -7" color="#9FD8EF"></a-sphere>
      <a-sphere dynamic-body radius="0.5" position="4 1.5 -7" color="#9FD8EF"></a-sphere>
    </a-scene>
  </body>
  <script src="https://codechangers.com/shoot-full.js"></script>
</html>
